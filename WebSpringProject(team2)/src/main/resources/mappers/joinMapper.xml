<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace = "com.koreadeal.web.DAO.JoinDAO">

<select id="idCheck" resultType="integer">
	SELECT 
		COUNT(*) 
	FROM 
		user_mst
	WHERE
		user_id = #{join_id};		
	</select>
</mapper>	
